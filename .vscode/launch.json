{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "DEBUG",
            "preLaunchTask": "build",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}\\src\\server.ts",
            "outFiles": [ 
                "${workspaceRoot}\\dist\\**\\*.js" 
            ],
            "env": {
                "NODE_ENV": "development",
                "PORT": "3135"
            },
            "console": "integratedTerminal",
            "runtimeArgs": [
                "--nolazy"
            ],
            "runtimeExecutable": null,
            "sourceMaps": true,
            "stopOnEntry": false,
            "args": []
        }
    ]
}